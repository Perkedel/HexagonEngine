			"a": 1,
			"b": 1,
			"g": 1,
			"r": 1,
			"type": "Color"
		},
		"ao": 1,
		"emission_energy": 1,
		"metallic": 0,
		"roughness": 1,
		"world": true
	},
	"seed": 0,
