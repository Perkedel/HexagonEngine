; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="HexagonEngine"
run/main_scene="res://HexagonEngineCore.tscn"
boot_splash/image="res://Sprites/HexagonEngineBootSplash.png"
boot_splash/bg_color=Color( 0, 0, 0.25098, 1 )
config/icon="res://Sprites/HexagonEngineSign.png"
config/auto_accept_quit=false
config/quit_on_go_back=false

[compression]

formats/zlib/compression_level=9
formats/gzip/compression_level=9

[debug]

gdscript/completion/autocomplete_setters_and_getters=true

[display]

window/size/width=1920
window/size/height=1080
window/dpi/allow_hidpi=true
mouse_cursor/custom_image="res://Sprites/GameCursorVacuumSkollNano.png"
mouse_cursor/tooltip_position_offset=Vector2( 30, 30 )
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

theme/use_hidpi=true

